{
  "name": "figura",
  "version": "6.0.1",
  "description": "View component for markup you already have.",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "author": "Ivan NikoliÄ‡ <niksy5@gmail.com> (http://ivannikolic.com)",
  "license": "MIT",
